<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%p] [%d] [%C] [%M] %m %n</pattern>
        </encoder>
    </appender>

    <logger name="com.ditcherj.acrcloud" level="ALL"/>

    <root level="ALL">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>